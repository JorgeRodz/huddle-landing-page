{
  "name": "huddle-landing-page",
  "version": "1.0.0",
  "description": "Landing page of a social media community",
  "scripts": {
    "pug:watch": "pug -w ./ -o ./ -P",
    "parcel:serve": "parcel serve ./index.html",
    "nra-dev": "npm-run-all --parallel pug:watch parcel:serve",
    "pug:compile": "pug ./index.pug -P",
    "parcel:build": "parcel build ./index.html",
    "nra-build": "npm-run-all pug:compile parcel:build"
  },
  "keywords": [],
  "author": "Jorge Alberto Rodriguez Flores",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0-beta.3.1",
    "autoprefixer": "^10.2.6",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.3.1",
    "pug-cli": "^1.0.0-alpha6"
  }
}
